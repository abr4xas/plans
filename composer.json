{
    "name": "abr4xas/plans",
    "description": "plans",
    "keywords": [
        "abr4xas",
        "plans"
    ],
    "homepage": "https://github.com/abr4xas/plans",
    "license": "MIT",
    "authors": [
        {
            "name": "angel cruz",
            "email": "angel@tepuilabs.dev",
            "homepage": "https://tepuilabs.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.4",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Abr4xas\\Plans\\": "src",
            "Abr4xas\\Plans\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abr4xas\\Plans\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "all": "composer psalm && composer test && composer test-coverage && composer format"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abr4xas\\Plans\\PlansServiceProvider"
            ],
            "aliases": {
                "Plans": "Abr4xas\\Plans\\PlansFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/abr4xas"
        },
        {
            "type": "other",
            "url": "https://tepuilabs.dev"
        }
    ]
}
